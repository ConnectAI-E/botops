{
  "name": "botops-feishu",
  "type": "module",
  "version": "0.0.13",
  "packageManager": "pnpm@8.14.0",
  "description": "botops feishu auto deploy module",
  "author": "River <laolei@forkway.c>",
  "license": "MIT",
  "homepage": "https://github.com/ConnectAI-E/botops",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ConnectAI-E/botops.git"
  },
  "bugs": "https://github.com/ConnectAI-E/botops/issues",
  "keywords": [
    "feishu",
    "cli",
    "botops"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "puppeteer-core": "^21.7.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/node": "^20.10.7",
    "@types/node-fetch": "^2.6.10",
    "bumpp": "^9.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "node-fetch": "^3.3.2",
    "pnpm": "^8.14.0",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
